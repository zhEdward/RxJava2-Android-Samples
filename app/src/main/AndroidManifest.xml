<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rxjava2.android.samples">

   <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <!--android:theme="@style/AppTheme"-->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        >
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".SimpleExampleActivity"
            android:label="@string/simple" />
        <activity
            android:name=".MapExampleActivity"
            android:label="@string/map" />
        <activity
            android:name=".ZipExampleActivity"
            android:label="@string/zip" />
        <activity
            android:name=".DisposableExampleActivity"
            android:label="@string/disposable" />
        <activity
            android:name=".TakeExampleActivity"
            android:label="@string/take" />
        <activity
            android:name=".TimerExample"
            android:theme="@style/MyAppTheme"
            android:label="@string/timer"
            android:parentActivityName=".IntervalExampleActivity"
            >

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".IntervalExampleActivity"
                />

            </activity>
        <activity
            android:name=".IntervalExampleActivity"
            android:label="@string/interval" />
        <activity
            android:name=".SingleObserverExampleActivity"
            android:label="@string/SingleObserver" />
        <activity
            android:name=".CompletableObserverActivity"
            android:label="@string/CompletableObserver" />
        <activity
            android:name=".FlowableExampleActivity"
            android:label="@string/Flowable" />
        <activity
            android:name=".ReduceExampleActivity"
            android:label="@string/reduce" />
        <activity
            android:name=".BufferExampleActivity"
            android:label="@string/buffer" />
        <activity
            android:name=".FilterExampleActivity"
            android:label="@string/filter" />
        <activity
            android:name=".SkipExampleActivity"
            android:label="@string/skip" />
        <activity
            android:name=".ScanExampleActivity"
            android:label="@string/scan" />
        <activity
            android:name=".ReplayExampleActivity"
            android:label="@string/replay" />
        <activity
            android:name=".ConcatExampleActivity"
            android:label="@string/concat" />
        <activity
            android:name=".MergeExampleActivity"
            android:label="@string/merge" />
        <activity
            android:name=".DeferExampleActivity"
            android:label="@string/defer" />
        <activity
            android:name=".DistinctExampleActivity"
            android:label="@string/distinct" />
        <activity
            android:name=".LastOperatorExampleActivity"
            android:label="@string/distinct" />

        <activity
            android:name=".SubjectExample"
            android:label="@string/subject"/>

        <activity
            android:name=".ThrottleLastExampleActivity"
            android:label="@string/throttle_last" />
        <activity
            android:name=".DebounceExampleActivity"
            android:label="@string/debounce" />
    </application>

</manifest>